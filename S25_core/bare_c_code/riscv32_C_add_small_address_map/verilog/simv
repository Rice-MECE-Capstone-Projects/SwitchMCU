S_0x559f5a639e00 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x559f5a64c170_0 .var "clk", 0 0;
v0x559f5a64c210_0 .var "reset", 0 0;
S_0x559f5a639f90 .scope module, "streamer" "riscv_command_streamer" 2 6, 3 1 0, S_0x559f5a639e00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 32 "instruction_o";
P_0x559f5a63a170 .param/l "MEM_SIZE" 0 3 1, +C4<00000000100110001001011010000000>;
v0x559f5a5fe490_0 .net "clk", 0 0, v0x559f5a64c170_0;  1 drivers
v0x559f5a64bc70_0 .var "instruction", 31 0;
o0x7fbde3082078 .functor BUFZ 32, C4<zzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzz>; HiZ drive
v0x559f5a64bd50_0 .net "instruction_o", 31 0, o0x7fbde3082078;  0 drivers
v0x559f5a64be40 .array "memory", 9999999 0, 31 0;
v0x559f5a64bf00_0 .var "pc", 31 0;
v0x559f5a64c030_0 .net "reset", 0 0, v0x559f5a64c210_0;  1 drivers
E_0x559f5a637830 .event posedge, v0x559f5a64c030_0, v0x559f5a5fe490_0;
    .scope S_0x559f5a639f90;
T_0 ;
    %vpi_call 3 11 "$readmemh", "program.hex", v0x559f5a64be40 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x559f5a64bf00_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x559f5a639f90;
T_1 ;
    %wait E_0x559f5a637830;
    %load/vec4 v0x559f5a64c030_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x559f5a64bf00_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x559f5a64bf00_0;
    %ix/load 5, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr 5;
    %ix/vec4 4;
    %load/vec4a v0x559f5a64be40, 4;
    %store/vec4 v0x559f5a64bc70_0, 0, 32;
    %load/vec4 v0x559f5a64bf00_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %vpi_call 3 20 "$display", "PC: %h, Instruction: %h, word in processor %h", v0x559f5a64bf00_0, v0x559f5a64bc70_0, S<0,vec4,u32> {1 0 0};
    %load/vec4 v0x559f5a64bf00_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x559f5a64bf00_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x559f5a639e00;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559f5a64c170_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x559f5a64c170_0;
    %inv;
    %store/vec4 v0x559f5a64c170_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x559f5a639e00;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559f5a64c210_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559f5a64c210_0, 0, 1;
    %delay 20, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_3;
